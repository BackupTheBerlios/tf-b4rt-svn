<table width="100%" border="0" cellpadding="0" cellspacing="0" style="margin-top:1px;margin-bottom:1px;"><tr>
<td bgcolor="<tmpl_var name="bgcolor">" width="<tmpl_var name="percent">%">
<tmpl_if name="percent" op=">" value="50">
	<div class="tinypercent" align="center"
		<tmpl_if name="percent" value="100">
			 style="background:#FF0000;">
		<tmpl_else>
			>
		</tmpl_if>
		<tmpl_var name="percent">%<tmpl_var name="text">
	</div>
</tmpl_if>
</td>
<td bgcolor="#000000" width="<tmpl_var name="percent2">%">
	<tmpl_if name="percent" op="<" value="50">
		<div class="tinypercent" align="center" style="color:<tmpl_var name="bgcolor">
			<tmpl_if name="percent" value="0">
				; background:#000000;">
			<tmpl_else>
				;">
			</tmpl_if>
			<tmpl_var name="percent">%<tmpl_var name="text">
		</div>
	</tmpl_if>
</td>
</tr></table>