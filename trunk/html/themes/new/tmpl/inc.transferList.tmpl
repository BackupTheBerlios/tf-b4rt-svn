<table class="sortable" id="transfer_table">
	<tmpl_if name="are_user_transfer">
	<!--	<tmpl_include name="inc.transferListHead.tmpl">		-->
			<tmpl_loop name="arUserTorrent">
				
				
				
				<div id="upload">
					<div id="uploadLast"></div>
					<div id="uploadFirst">
						<span class="upload">
							<a href="javascript:slider('<tmpl_var name="entry">','toggle')">
								<img src="themes/default/images/locked.gif" alt="<tmpl_var name="_NOTOWNER">" height="5">
							</a>
							<img src="themes/default/images/<tmpl_var name="hd_image">" title="<tmpl_var name="hd_title"><tmpl_var name="entry">" height="5">
							<tmpl_if name="enable_metafile_download">
								<a href="dispatcher.php?action=metafileDownload&transfer=<tmpl_var name="url_entry">">
									<img src="themes/default/images/down.gif" title="Download Metafile" height="5">
								</a>
							</tmpl_if>
							<a href="index.php?iid=dir&dir=<tmpl_var name="url_path">" onclick="return actionClick(true)">
								<img src="themes/default/images/datadir.gif" title="<tmpl_var name="datapath">" height="5">
							</a>
							<a class="upload" href="JavaScript:showTransfer('index.php?iid=<tmpl_var name="twd">&transfer=<tmpl_var name="url_entry">')">
								<tmpl_var name="displayname">
							</a>
							<tmpl_if name="is_owner">
								<a href="JavaScript:showTransfer('index.php?iid=transferSettings&transfer=<tmpl_var name="url_entry">')"><img src="themes/default/images/properties.png" alt="Transfer Settings" height="5"></a>
								<tmpl_if name="transferRunning" value="1">
									<img src="themes/default/images/blank.gif" height="5">
									<a href="dispatcher.php?action=stop&transfer=<tmpl_var name="url_entry">" onclick="return actionClick(true)"><img src="themes/default/images/bulkstop.gif" alt="<tmpl_var name="_STOPTRANSFER">" height="5"></a>
									<img src="themes/default/images/delete_off.gif" height="5">
									<tmpl_if name="enable_multiops">
										<input type="checkbox" name="transfer[]" value="<tmpl_var name="url_entry">" onclick="ajax_updateState = 0;">
									</tmpl_if>
								<tmpl_else>
									<tmpl_if name="transferowner" value="n/a">
										<img src="themes/default/images/blank.gif" height="5">
										<img src="themes/default/images/run_off.gif" alt="<tmpl_var name="_NOTOWNER">" height="5">
									<tmpl_else>
										<tmpl_if name="transferRunning" value="3">
											<img src="themes/default/images/blank.gif" height="5">
											<a href="dispatcher.php?action=deQueue&transfer=<tmpl_var name="url_entry">" onclick="return actionClick(true)"><img src="themes/default/images/queued.gif" alt="<tmpl_var name="_DELQUEUE">" height="5"></a>
										<tmpl_else>
											<tmpl_if name="is_no_file">
												<tmpl_if name="show_run" value="1">
													<a href="#" onclick="showTransfer('index.php?iid=transferControl&transfer=<tmpl_var name="url_entry">')"><img src="themes/default/images/bulkstart.gif" alt="<tmpl_var name="_RUNTRANSFER">" height="5"></a>
												<tmpl_else>
													<a href="#" onclick="showTransfer('index.php?iid=transferControl&transfer=<tmpl_var name="url_entry">')"><img src="themes/default/images/seed_on.gif" alt="<tmpl_var name="_SEEDTRANSFER">" height="5"></a>
												</tmpl_if>
												<tmpl_if name="show_run" value="1">
													<a href="dispatcher.php?action=start&transfer=<tmpl_var name="url_entry">" onclick="return actionClick(true)"><img src="themes/default/images/bulkresume.gif" alt="<tmpl_var name="_RUNTRANSFER">" height="5"></a>
												<tmpl_else>
													<tmpl_if name="clientType" value="torrent">
														<a href="dispatcher.php?action=start&transfer=<tmpl_var name="url_entry">" onclick="return actionClick(true)"><img src="themes/default/images/seed_on.gif" alt="<tmpl_var name="_SEEDTRANSFER">" height="5"></a>
													<tmpl_else>
														<img src="themes/default/images/run_off.gif" alt="Done" height="5">
													</tmpl_if>
												</tmpl_if>
											<tmpl_else>
												<img src="themes/default/images/blank.gif" height="5">
												<img src="themes/default/images/run_off.gif" alt="<tmpl_var name="_STOPPING">" height="5">
											</tmpl_if>
										</tmpl_if>
									</tmpl_if>
									<tmpl_if name="is_no_file">
										<a href="dispatcher.php?action=delete&transfer=<tmpl_var name="url_entry">" onclick="return actionConfirm('Delete <tmpl_var name="entry"> ?')"><img src="themes/default/images/delete_on.gif" alt="<tmpl_var name="_DELETE">" height="5"></a>
									<tmpl_else>
										<img src="themes/default/images/delete_off.gif" alt="<tmpl_var name="_STOPPING">" height="5">
									</tmpl_if>
									<tmpl_if name="enable_multiops">
										<input type="checkbox" name="transfer[]" value="<tmpl_var name="url_entry">" onclick="ajax_updateState = 0;">
									</tmpl_if>
								</tmpl_if>
							<tmpl_else>
								<img src="themes/default/images/locked.gif" alt="<tmpl_var name="_NOTOWNER">" height="5">
								<img src="themes/default/images/locked.gif" alt="<tmpl_var name="_NOTOWNER">" height="5">
								<img src="themes/default/images/locked.gif" alt="<tmpl_var name="_NOTOWNER">" height="5">
								<input type="checkbox" disabled="disabled">
							</tmpl_if>
						</span>
					</div>
				</div>
				
				
				
				
				
				<div id="<tmpl_var name="entry">" style="display: none; height: 0px;">
					
					<div class="uploadClass">
							
							
					<tmpl_if name="settings_0">
						<a id="closeLink" href="index.php?iid=message&to_user=<tmpl_var name="transferowner">">
							<tmpl_var name="transferowner">
						</a>
					</tmpl_if>
					<tmpl_if name="settings_1">
						<a id="closeLink" href="JavaScript:showTransfer('index.php?iid=<tmpl_var name="twd">&transfer=<tmpl_var name="url_entry">')">
							<tmpl_var name="format_af_size">
						</a>
					</tmpl_if>
					<tmpl_if name="settings_2">
						<a id="closeLink" href="JavaScript:showTransfer('index.php?iid=<tmpl_var name="twd">&transfer=<tmpl_var name="url_entry">')">
							<tmpl_var name="format_downtotal">
						</a>
					</tmpl_if>
					<tmpl_if name="settings_3">
						<a id="closeLink" href="JavaScript:showTransfer('index.php?iid=<tmpl_var name="twd">&transfer=<tmpl_var name="url_entry">')">
							<tmpl_var name="format_uptotal">
						</a>
					</tmpl_if>
					<tmpl_if name="settings_4">
						<a id="closeLink" href="JavaScript:showTransfer('index.php?iid=<tmpl_var name="twd">&transfer=<tmpl_var name="url_entry">')">
							<tmpl_var name="statusStr">
						</a>
					</tmpl_if>
					<tmpl_if name="settings_5">
						<tmpl_if name="graph_width" value="-1">
							<a id="closeLink" href="JavaScript:showTransfer('index.php?iid=<tmpl_var name="twd">&transfer=<tmpl_var name="url_entry">')"><tmpl_var name="percentage"></a>
						<tmpl_else>
							<tmpl_if name="transferRunning" op="<" value="2">
								<!--SHOW BAR -->
							</tmpl_if>
						</tmpl_if>
					</tmpl_if>
					<tmpl_if name="settings_6">
						<tmpl_if name="transferRunning" value="1">
							<a id="closeLink" href="JavaScript:showTransfer('index.php?iid=<tmpl_var name="twd">&transfer=<tmpl_var name="url_entry">')">
								<tmpl_var name="down_speed">
							</a>
						</tmpl_if>
					</tmpl_if>
					<tmpl_if name="settings_7">
						<tmpl_if name="transferRunning" value="1">
							<a id="closeLink" href="JavaScript:showTransfer('index.php?iid=<tmpl_var name="twd">&transfer=<tmpl_var name="url_entry">')">
								<tmpl_var name="up_speed">
							</a>
						</tmpl_if>
					</tmpl_if>
					<tmpl_if name="settings_8">
						<tmpl_if name="transferRunning" value="1">
							<tmpl_if name="seeds" op="!=" value="">
								<a id="closeLink" href="JavaScript:showTransfer('index.php?iid=<tmpl_var name="twd">&transfer=<tmpl_var name="url_entry">')">
									<tmpl_var name="seeds">
								</a>
							</tmpl_if>
						</tmpl_if>
					</tmpl_if>
					<tmpl_if name="settings_9">
						<tmpl_if name="transferRunning" value="1">
							<tmpl_if name="peers" op="!=" value="">
								<a id="closeLink" href="JavaScript:showTransfer('index.php?iid=<tmpl_var name="twd">&transfer=<tmpl_var name="url_entry">')">
									<tmpl_var name="peers">
								</a>
							</tmpl_if>
						</tmpl_if>
					</tmpl_if>
					<tmpl_if name="settings_10">
						<a id="closeLink" href="JavaScript:showTransfer('index.php?iid=<tmpl_var name="twd">&transfer=<tmpl_var name="url_entry">')">
							<tmpl_var name="estTime">
						</a>
					</tmpl_if>
					<tmpl_if name="settings_11">
						<a id="closeLink" href="JavaScript:showTransfer('index.php?iid=<tmpl_var name="twd">&transfer=<tmpl_var name="url_entry">')">
							<tmpl_var name="client">
						</a>
					</tmpl_if>
							
				</div>
				<div id="uploadBottom"></div>
			<div id="uploadBottomEnd"></div>
		</div>
			</tmpl_loop>
			
	</tmpl_if>
	
	
</table>




