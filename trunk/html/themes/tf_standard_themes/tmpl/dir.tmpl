							<a href="index.php?iid=dir&down=<tmpl_var name="urlencode1">"><img src="<tmpl_var name="image">" width="16" height="16" alt="<tmpl_var name="entry">" border="0"></a>
							<a href="index.php?iid=dir&down=<tmpl_var name="urlencode1">"><tmpl_var name="entry"></a>
						<tmpl_else>
							<img src="<tmpl_var name="image">" width="16" height="16" alt="<tmpl_var name="entry">" border="0">
							<tmpl_var name="entry">
						</tmpl_if>
					</td>
					<td align="right">
						<tmpl_var name="fileSize"> KB</td>
					<tmpl_if name="enable_dirstats">
						<td width=140><tmpl_var name="date"></td>
					<tmpl_else>
						<td><tmpl_var name="date"></td>
					</tmpl_if>
					<td align="right" nowrap>
						<tmpl_if name="enable_rename">
							<a href="JavaScript:renameFolder('index.php?iid=renameFolder&dir=<tmpl_var name="urlencode2">&file=<tmpl_var name="urlencode3">&start=true')"><img src="themes/tf_standard_themes/images/rename.gif" width=16 height=16 title="<tmpl_var name="_DIR_REN_LINK">" border=0></a>
						</tmpl_if>
						<tmpl_if name="enable_move">
							<a href="JavaScript:moveFile('index.php?iid=move&path=<tmpl_var name="urlencode1">&start=true')"><img src="themes/tf_standard_themes/images/_move.gif" width=16 height=16 title="<tmpl_Var name="_DIR_MOVE_LINK">" border=0></a>
						</tmpl_if>
						<tmpl_if name="is_rar" value="1">
							<a href="javascript:UncompDetails('index.php?iid=uncomp&file=<tmpl_var name="urlencode1">&dir=<tmpl_var name="urlencode2">&type=rar')"><img src="themes/tf_standard_themes/images/rar_enabled.gif" width=16 height=16 title="Unrar" border=0></a>
