						</tmpl_if>
						<tmpl_if name="is_nfo">
							<a href="index.php?iid=viewnfo&path=<tmpl_var name="urlencode4">"><img src="themes/tf_standard_themes/images/view_nfo.gif" width=16 height=16 title="View <tmpl_var name="entry">" border=0></a>
						</tmpl_if>
						<tmpl_if name="enable_maketorrent">
							<a href="JavaScript:MakeTorrent('index.php?iid=maketorrent&path=<tmpl_var name="urlencode1">')"><img src="themes/tf_standard_themes/images/make.gif" width=16 height=16 title="Make Torrent" border=0></a>
						</tmpl_if>
						<tmpl_if name="enable_file_download">
							<a href="index.php?iid=dir&down=<tmpl_var name="urlencode1">"><img src="themes/tf_standard_themes/images/download_owner.gif" width=16 height=16 title="Download" border=0></a>
						</tmpl_if>
						<tmpl_if name="aclDelete">
							<a href="index.php?iid=dir&del=<tmpl_var name="urlencode1">" onclick="return ConfirmDelete('<tmpl_var name="addslashes1">')"><img src="themes/tf_standard_themes/images/delete_on.gif" width=16 height=16 title="<tmpl_var name="_DELETE">" border=0></a>
							<input type="checkbox" name="file[]" value="<tmpl_var name="urlencode1">">
						</tmpl_if>
					</td>
				</tr>
			</tmpl_loop>
		</tmpl_if>
		<input type="hidden" name="action" value="fileDelete" />
		<tr>
			<td align="right" colspan="4">
				<a href="javascript:document.multidir.submit()" onclick="return ConfirmDelete('Multiple Files')"><img src="themes/tf_standard_themes/images/delete_on.gif" title="Delete" border="0" height="16" width="16"></a>
				<input type="checkbox" onclick="checkCheck(this);" />
			</td>
		</tr>
	</form>
</table>
<tmpl_if name="enable_dirstats">
