					</td>
					<tmpl_if name="enable_dirstats">
						<td align="right"><tmpl_var name="dusize">B</td>
						<td width=140><tmpl_var name="date"></td>
					<tmpl_else>
						<td></td>
						<td></td>
					</tmpl_if>
					<td align="right" nowrap>
						<tmpl_if name="enable_rename">
							<a href="JavaScript:renameFolder('index.php?iid=renameFolder&dir=<tmpl_var name="urlencode2">&file=<tmpl_var name="urlencode3">&start=true')"><img src="themes/tf_standard_themes/images/rename.gif" width=16 height=16 title="<tmpl_var name="_DIR_REN_LINK">" border=0></a>
						</tmpl_if>
