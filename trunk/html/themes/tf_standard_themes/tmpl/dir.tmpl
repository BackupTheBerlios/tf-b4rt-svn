<tmpl_include name="inc.head.tmpl">
<tmpl_var name="head">
<script language="JavaScript">
function MakeTorrent(name_file) {
	window.open (name_file,'_blank','toolbar=no,location=no,directories=no,status=no,menubar=no,scrollbars=yes,resizable=no,width=600,height=430')
}
function ConfirmDelete(file) {
	return confirm("<tmpl_var name="_ABOUTTODELETE">: " + file)
}
function checkCheck(thisIn) {
	var form = thisIn.form, i = 0;
	for(i=0; i < form.length; i++)
	{
		if( form[i].type == 'checkbox' && form[i].name != 'checkall')
		{
			form[i].checked = thisIn.checked;
		}
	}
}
function UncompDetails(URL) {
	window.open (URL,'_blank','toolbar=no,location=no,directories=no,status=no,menubar=no,scrollbars=yes,resizable=no,width=600,height=300');
}
</script>
<tmpl_if name="enable_rename">
	<script type="text/javascript" language="JavaScript">
		var popUpWin=0;
		function renameFolder(name_file) {
			if(popUpWin) {
				if(!popUpWin.closed) popUpWin.close();
			}
			popUpWin = open(name_file,'_blank','toolbar=no,location=0,directories=no,status=no,menubar=no,scrollbars=yes,resizable=no,width=600,height=430')
		}
	</script>
</tmpl_if>
<tmpl_if name="enable_move">
	<script type="text/javascript" language="JavaScript">
		var popUpWin=0;
		function moveFile(name_file) {
			if(popUpWin) {
				if(!popUpWin.closed) popUpWin.close();
			}
			popUpWin = open(name_file,'_blank','toolbar=no,location=0,directories=no,status=no,menubar=no,scrollbars=yes,resizable=no,width=600,height=430');
		}
	</script>
</tmpl_if>
<tmpl_var name="driveSpaceBar">
<br>
<a href="<tmpl_var name="parentURL">"><img src="themes/tf_standard_themes/images/up_dir.gif" width=16 height=16 title="<tmpl_var name="_BACKTOPARRENT">" border=0>[<tmpl_var name="_BACKTOPARRENT">]</a>
<table cellpadding="2"" width="740">
	<form action="index.php?iid=dir&dir=<tmpl_var name="dir">&multidel=1" method="post" name="multidir">
		<tmpl_if name="dirlist">
