							<input type="checkbox" name="file[]" value="<tmpl_var name="urlencode1">">
						</tmpl_if>
					</td>
				</tr>
			</tmpl_loop>
		</tmpl_if>
		<tmpl_if name="filelist">
			<tmpl_loop name="filelist">
